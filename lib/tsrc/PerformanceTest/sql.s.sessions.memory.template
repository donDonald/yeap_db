CREATE TABLE %tableName%
(
    id             INT         NOT NULL AUTO_INCREMENT,
    ts             TIMESTAMP   NOT NULL DEFAULT CURRENT_TIMESTAMP,
    user           CHAR(32)    NOT NULL,
    length         INT         NOT NULL,
    PRIMARY KEY    (id),
    INDEX          id(id),
    INDEX          ts(ts),
    INDEX          user(user)
)
ENGINE = MEMORY;
